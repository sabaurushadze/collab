<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="EditTextOutlined">
        <item name="android:background">@drawable/et_border</item>
        <item name="android:textColor">@color/black2</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
    </style>

    <style name="EditTextFilled">
        <item name="android:background">@drawable/et_border_filled</item>
        <item name="android:backgroundTint">@color/white3</item>
        <item name="android:textColor">@color/black2</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
    </style>

    <style name="ButtonOutlined">
        <item name="android:textAllCaps">false</item>
        <item name="android:background">@drawable/btn_round_outlined_6dp</item>
        <item name="android:textColor">@color/black2</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
    </style>

    <style name="ButtonPrimaryFilled">
        <item name="android:background">@drawable/btn_round_6dp</item>
        <item name="android:textAllCaps">false</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">14sp</item>
        <item name="android:paddingStart">16dp</item>
        <item name="android:paddingEnd">16dp</item>
    </style>

    <style name="App.Custom.Indicator" parent="Widget.Material3.BottomNavigationView.ActiveIndicator">
        <item name="android:color">@android:color/transparent</item>
    </style>

    <style name="ShapeAppearance.Circle" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>